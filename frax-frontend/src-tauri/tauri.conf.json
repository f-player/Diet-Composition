{
  "productName": "frax-frontend",
  "version": "0.1.0",
  "identifier": "org.fraxcalculator.desktop",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "https://localhost:3000",
    "beforeDevCommand": "npm run dev", 
    "beforeBuildCommand": "npm run build:tauri"
  },
  "app": {
    "windows": [
      {
        "title": "frax-frontend",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "http": {
      "scope": ["http://127.0.0.1:*"],
      "dangerousAcceptAnyServerCertificate": true
    }
  }
}